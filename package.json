{
  "name": "nextjs-template",
  "private": true,
  "version": "0.0.1",
  "templateVersion": "1.2.4",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3010",
    "dev:turbo": "cross-env NODE_ENV=development next dev --turbo --show-all -p 3010",
    "build": "next build",
    "start": "cross-env NODE_ENV=production next start -p 3003",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "test": "cross-env NODE_ENV=test jest --env=jsdom --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --env=jsdom --coverage",
    "style:check": "prettier --check ./**/*.{ts,tsx}",
    "style:fix": "prettier --write ./**/*.{ts,tsx}",
    "type-check": "tsc -p tsconfig.json --noEmit --pretty",
    "lint:eslint:fix": "yarn eslint . --ext .ts,.tsx --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "resolutions": {
    "styled-components": "5.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    },
    "commitizenEmoji": {
      "conventionalFormat": true
    }
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.5.6",
    "@next/bundle-analyzer": "13.1.2",
    "@reduxjs/toolkit": "1.9.1",
    "antd": "5.1.5",
    "apisauce": "2.1.6",
    "babel-plugin-import": "1.13.6",
    "cross-env": "7.0.3",
    "history": "5.3.0",
    "isomorphic-fetch": "3.0.0",
    "less": "4.1.3",
    "lodash-es": "4.17.21",
    "next": "13.1.2",
    "next-compose-plugins": "2.2.1",
    "next-pwa": "5.6.0",
    "next-redux-wrapper": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.2.5",
    "react-redux": "8.0.5",
    "styled-components": "5.3.6",
    "styled-media-query": "2.1.2",
    "util": "0.12.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/jest": "29.2.6",
    "@types/lodash": "4.14.191",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.11.18",
    "@types/prop-types": "15.7.5",
    "@types/react": "18.0.27",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@welldone-software/why-did-you-render": "7.0.1",
    "commitizen": "4.3.0",
    "commitizen-emoji": "1.0.5",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-react": "7.32.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "next-router-mock": "0.8.0",
    "prettier": "2.8.3",
    "prop-types": "15.8.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4"
  }
}
